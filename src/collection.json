{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "simple-module": {
      "description": "Creates a full simple module.",
      "factory": "./simple-module/index#simpleModule",
      "schema": "./simple-module/schema.json",
      "aliases": ["sm"]
    },
    "service": {
      "description": "Creates a full service.",
      "factory": "./service/index#service",
      "schema": "./service/schema.json",
      "aliases": ["svc"]
    }
  }
}
